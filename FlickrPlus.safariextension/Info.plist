<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Author</key>
	<string>Rob Wilkerson</string>
	<key>CFBundleDisplayName</key>
	<string>FlickrPlus</string>
	<key>CFBundleIdentifier</key>
	<string>org.robwilkerson.safari.flickrplus</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleShortVersionString</key>
	<string>0.7.0</string>
	<key>CFBundleVersion</key>
	<string>9</string>
	<key>Chrome</key>
	<dict>
		<key>Global Page</key>
		<string>global.html</string>
	</dict>
	<key>Content</key>
	<dict>
		<key>Scripts</key>
		<dict>
			<key>End</key>
			<array>
				<string>links.js</string>
			</array>
		</dict>
		<key>Stylesheets</key>
		<array>
			<string>flickrplus.css</string>
		</array>
		<key>Whitelist</key>
		<array>
			<string>http://*.flickr.com/photos/*</string>
		</array>
	</dict>
	<key>Description</key>
	<string>A collection of tools designed to improve the Flickr experience.</string>
	<key>ExtensionInfoDictionaryVersion</key>
	<string>1.0</string>
	<key>Permissions</key>
	<dict>
		<key>Website Access</key>
		<dict>
			<key>Allowed Domains</key>
			<array>
				<string>*.flickr.com</string>
				<string>flickr.com</string>
			</array>
			<key>Include Secure Pages</key>
			<true/>
			<key>Level</key>
			<string>Some</string>
		</dict>
	</dict>
	<key>Update Manifest URL</key>
	<string>http://robwilkerson.org/_resources/projects/safari/extensions/FlickrPlus-update.plist</string>
	<key>Website</key>
	<string>http://codaset.com/robwilkerson/flickrplus/wiki/home</string>
</dict>
</plist>
